{
  "name": "miniapp-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "基于Taro的小程序项目模板",
  "author": "",
  "scripts": {
    "postinstall": "husky install",
    "eslint": "eslint ./ --fix",
    "stylelint": "stylelint \"src/**/*.{css,less,scss,sass,styl}\" --fix",
    "lint": "npm run eslint && npm run stylelint",
    "dev:weapp": "cross-env API_URL=http://xxx.xxx.xxx:8000 ASSET_URL=http:http://xxx.xxx.xxx:3000 npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev": "npm run dev:weapp",
    "test": "API_URL=https://a.b.c ts-node test",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "gen:api": "a2s generate -y",
    "upload": "minat upload",
    "up:test": "cross-env API_URL=http://xxx.xxx.xxx:8000/ ASSET_URL=http:http://xxx.xxx.xxx:3000 npm run build:weapp && minat upload",
    "up:prod": "cross-env API_URL=https://xxx.xxx.xxx/ ASSET_URL=http:http://xxx.xxx.xxx:3000 npm run build:weapp && minat upload"
  },
  "dependencies": {
    "@ahooksjs/use-request": "^2.8.7",
    "@babel/runtime": "^7.14.6",
    "@react-hook/debounce": "^4.0.0",
    "@tarojs/components": "3.2.13",
    "@tarojs/react": "3.2.13",
    "@tarojs/runtime": "3.2.13",
    "@tarojs/taro": "3.2.13",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "taro-ui": "^3.0.0-alpha.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@tarojs/cli": "3.2.13",
    "@tarojs/mini-runner": "3.2.13",
    "@tarojs/webpack-runner": "3.2.13",
    "@types/baretest": "^2.0.0",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@zidong/a2s": "^0.0.7",
    "babel-preset-taro": "3.2.13",
    "baretest": "^2.0.0",
    "cross-env": "^7.0.3",
    "devmoji": "^2.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.2.13",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "minat": "^0.1.1",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.4"
  },
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "less"
  }
}
